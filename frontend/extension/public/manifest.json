{
    "name": "Impressionist",
    "version": "1.02",
    "description": "An extension to do impressions of actors in your favorite flix.",
    "options_page": "options.html",
    "permissions": [
      "http://localhost/",
      "https://impressionist-user-db-api-east-1.crossley.tech/",
      "https://impressionist-content-db-api-east-1.crossley.tech/",
      "activeTab",
      "declarativeContent",
      "storage",
      "identity",
      "tabs"
    ],
    "background": {
      "scripts": ["scripts/background.js"],
      "persistent": false
    },
    "content_scripts": [
      {
        "matches": ["https://www.netflix.com/watch/*"],
        "css": ["css/contentScript.css"],
        "js": ["scripts/socket.io.slim.js", "scripts/contentScript.js"]
      }
    ],
    "web_accessible_resources": ["scripts/netflixApiScript.js", "scripts/dictionary.js"],
    "page_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/logo16.png",
        "32": "images/logo32.png",
        "48": "images/logo48.png",
        "128": "images/logo128.png"
      }
    },
    "icons": {
      "16": "images/logo16.png",
      "32": "images/logo32.png",
      "48": "images/logo48.png",
      "128": "images/logo128.png"
    },
    "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
    "oauth2": {
      "client_id": "27736832294-lglt2rri6n1vaohla22rb2bvlpkechf6.apps.googleusercontent.com",
      "scopes": ["https://www.googleapis.com/auth/userinfo.email", "https://www.googleapis.com/auth/userinfo.profile"]
    },
    "manifest_version": 2
  }

{
    "name": "Impressionist",
    "version": "1.0",
    "description": "an extension to make impressions of actors in your favorite video content.",
    "options_page": "options.html",
    "permissions": [
      "http://localhost/",
      "http://ec2-18-223-101-151.us-east-2.compute.amazonaws.com/",
      "http://ec2-3-82-150-208.compute-1.amazonaws.com",
      "activeTab", 
      "declarativeContent", 
      "storage"
    ],
    "background": {
      "scripts": ["scripts/background.js"],
      "persistent": false
    },
    "content_scripts": [
      {
        "matches": ["https://www.netflix.com/watch/*"],
        "css": ["css/contentScript.css"],
        "js": ["scripts/socket.io.slim.js", "scripts/contentScript.js"]
      }
    ],
    "web_accessible_resources": ["scripts/netflixApiScript.js"],
    "page_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/logo16.png",
        "32": "images/logo32.png",
        "48": "images/logo48.png",
        "128": "images/logo128.png"
      }
    },
    "icons": {
      "16": "images/logo16.png",
      "32": "images/logo32.png",    
      "48": "images/logo48.png",
      "128": "images/logo128.png"
    },
    "manifest_version": 2
  }